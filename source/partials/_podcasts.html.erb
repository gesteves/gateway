<section class="section" aria-labelledby="podcasts">
  <h2 id="podcasts">Podcasts</h2>
  <ol>
    <% data.podcasts.links.reverse.each do |link| %>
      <li>
        <h3><%= link_to remove_widows(link.title), link.url %></h3>
        <p><%= remove_widows link.description %></p>
      </li>
    <% end %>
  </ol>
</section>
